{
  "os": [
    "ubuntu-latest"
  ],
  "compatible": [
    "backward",
    "forward"
  ],
  "include": [
    {
      "compatible": "backward",
      "azure_cli_version": "latest",
      "destroy": true,
      "ignore_destroy_failure": false,
      "pin_provider_versions": true,
      "terraform_apply": true,
      "terraform_version": "latest",
      "upgrade_azure_cli": false
    },
    {
      "compatible": "forward",
      "azure_cli_version": "latest",
      "destroy": true,
      "ignore_destroy_failure": true,
      "pin_provider_versions": false,
      "terraform_apply": true,
      "terraform_version": "latest",
      "upgrade_azure_cli": true
    }
  ]
}